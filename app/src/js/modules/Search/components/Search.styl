.Search
  width: 850px
  margin: 0 auto
  border: 1px solid #DDDDDD
  border-radius: 32px
  color: #222222
  height: 66px
  position: relative
  box-shadow: 0px 16px 32px rgba(0, 0, 0, 0.15), 0px 3px 8px rgba(0, 0, 0, 0.1)
  background-color: #FFF
  text-align: center
  display: table
  overflow: hidden

  .value
    position: absolute
    top: 20px
    font-size: 20px
    font-weight: bold
    width: 32px
    right: 32px

